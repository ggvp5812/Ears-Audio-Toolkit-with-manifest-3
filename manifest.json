{
  "manifest_version": 3,
  "name": "Ears: Bass Boost(Unofficial port) ",
  "short_name": "Ears",
  "description": "EQ any audio you find on the web, live! Crank the bass, dim the highs, up the vocals: all with Ears!(Unofficial port to manifest_version 3) ",
  "version": "1.4.1",
  "author": "Kevin King",
  "icons": {
    "16": "ears16.png",
    "32": "ears32.png",
    "48": "ears48.png",
    "64": "ears64.png",
    "128": "ears128.png"
  },
  "action": {
    "default_icon": "ears128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabCapture",
    "tabs",
    "storage",
    "offscreen"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
